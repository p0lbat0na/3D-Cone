    <style>
        .baton {
            border: none;
            border-radius: 15px;
        }

        .sign {
            display: flex;
            flex-wrap: wrap;
            /*background: white url(/img/Group.png)no-repeat 24px center;
        background-size: 20px,18px;*/
            top: -3px;
            padding: 10px 0px 0px 15px;
            border-radius: 32px;
            vertical-align: bottom;
            width: 250px;
            height: 56px;
            border: 1px solid #E0E3EB;
            margin-top: -9px;
            margin-bottom: 20px;
        }

        .serch_textbox {
            border: 0;
            font-size: 20px;
            outline: none;
            height: 46px;
            top: 50px;
            margin-top: 5px;
            background-color: transparent;
        }

        .search_button {
            font-family: 'Lexend';
            font-style: normal;
            font-weight: 600;
            font-size: 11px;
            color: white;
            padding: 0;
            margin-left: -30px;
            margin-top: 5px;
            width: 70px;
            height: 40px;
            border: none;
            border-radius: 32px;
            /*background: #10B981 url(/img/search.png)no-repeat 69px 13px;
        background-size: 24px, 24px;*/
        }

        /*.search_button:hover, .search_button:active {
        background: #0c835b url(/img/search.png)no-repeat 69px 13px;
        }*/

        .fieldset {
            display: flex;
            vertical-align: top;
            justify-content: space-between;
            margin-top: -8px;
            margin-right: 0;
            width: 150px;
        }

        .text-field__label {
            position: relative;
            color: #888B97;
            font-size: 14px;
            pointer-events: none;
            left: 0px;
            margin-left: -200px;
            top: 0px;
            white-space: nowrap;
            overflow: hidden;
            line-height: 40px;
            transition: .3s;
        }

        .serch_textbox:focus ~ .text-field__label,
        .serch_textbox:not(:focus):valid ~ .text-field__label {
            top: -15px;
            font-size: 10px;
        }
    </style>
