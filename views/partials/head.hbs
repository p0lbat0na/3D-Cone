    <style>
        .intro_title {
            font-family: 'Lexend', sans-serif;

            text-transform: capitalize;
            color: #1B1C57;
            font-weight: 800;
            font-style: normal;
            font-size: 40px;
            width: 496px;
            line-height: 47px;
            margin-bottom: 24px;
        }

        strong {
            color: transparent;
            position: relative;
            /* We need double the stroke width because half of it gets covered up */
            -webkit-text-stroke: 3px #1B1C57;
            font-weight: 600;

        }

        .intro_title_white {
            color: white;
            position: absolute;
            left: 99px;
            -webkit-text-stroke: 0;
            font-weight: 600;
        }

        .btn {
            width: 45%;
            margin-bottom: 10px;
            min-width: 239px;
            margin-left: 20px;
            font-size: smaller;
        }

        .btn-small {

            margin-bottom: 10px;

            margin-left: 5px;
            font-size: smaller;
        }

        .social {
            margin: 10px;
            height: 110px;
            width: 105px;
            background-color: dimgray;
            display: flex;
            justify-content: space-between;
            flex-wrap: wrap;
            border-radius: 15px;
            -webkit-transition: all 1s ease;
            -moz-transition: all 1s ease;
            -o-transition: all 1s ease;
            transition: all 1s ease;
            padding: 1%;
        }

        .social:hover {
            margin: 10px;
            height: 200px;
            width: 200px;
            padding: 2%;
            padding-bottom: 0;

            background-color: dimgray;
            display: flex;
            justify-content: space-between;
            flex-wrap: wrap;
            border-radius: 50%;
            -webkit-transition: all 1s ease;
            -moz-transition: all 1s ease;
            -o-transition: all 1s ease;
            transition: all 1s ease;
        }

        .btn-social {
            margin: 0%;
            -webkit-transition: all 2s ease;
            -moz-transition: all 2s ease;
            -o-transition: all 2s ease;
            transition: all 2s ease;
        }

        .phone-container {
            background-color: aqua;
            bottom: 10px;
            right: 10px;
            margin: 5%;
            margin-bottom: 0%;
            margin-right: 40px;
            position: relative;
            -webkit-transition: all 2s ease;
            -moz-transition: all 2s ease;
            -o-transition: all 2s ease;
            transition: all 2s ease;
        }

        .social:hover .btn-social {
            animation-delay: 500ms;
            margin: 10%;
            -webkit-transition: all 0.1s ease;
            -moz-transition: all 0.1s ease;
            -o-transition: all 0.1s ease;
            transition: all 0.9s ease;

        }

        .social:hover .phone-container {
            animation-delay: 500ms;
            margin: 15%;
            -webkit-transition: all 2s ease;
            -moz-transition: all 2s ease;
            -o-transition: all 2s ease;
            transition: all 2s ease;
        }

        .git {
            position: inherit;
            top: 10px;
            left: 10px;
        }

        .hh {
            position: inherit;
            top: 10px;
            right: 10px;
        }

        .mail {
            position: inherit;
            bottom: 10px;
            left: 10px;
        }



        .phone-btn {
            position: inherit;

            z-index: 20;
            max-width: 50px;
        }

        .phone-text {
            display: block;
            height: 40px;
            margin-top: -40px;
            width: 90%;
            visibility: visible;
            z-index: 19;
            background-color: aliceblue;
        }

        .phone-btn:hover {
            position: relative;
        }

        .phone-container:hover .phone-text:after {
            position: fixed;
            top: 20px;
            transform: scale(2, 0.5);
            width: 40px;
            margin-bottom: 30px;
            margin-top: 0px;
            margin-left: 20px;
            content: '8(987)291-63-53';

            -webkit-transition: all 0.3s ease;
            -moz-transition: all 0.3s ease;
            -o-transition: all 0.3s ease;
        }

        .mail:hover+.phone-text:after {
            margin-bottom: 30px;
            margin-top: -13px;
            margin-left: 20px;
            content: 'arsenya.popov.03@bk.ru';
        }

        .flow-text {
            font-size: small;
        }

        .s6 {
            margin-left: 20px;
        }
